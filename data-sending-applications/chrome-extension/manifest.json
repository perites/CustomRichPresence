{
  "manifest_version": 3,
  "name": "CustomRichPresence",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://animejoy.site/*",
        "https://www.geoguessr.com/*",
        "https://anime-joy.online/*"
      ],
      "js": [
        "content_CRP.js"
      ]
    }
  ],
  "host_permissions": [
    "https://www.geoguessr.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "nativeMessaging"
  ],
  "icons": {
    "48": "logo.png",
    "128": "logo.png"
  }
}

	
